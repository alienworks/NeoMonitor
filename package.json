{
  "name": "neo-seed-nodes-monitor",
  "version": "1.3.0",
  "private": true,
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,vue}": [
      "prettier --write",
      "vue-cli-service lint"
    ],
    "*.{md,json}": [
      "prettier --write"
    ]
  },
  "scripts": {
    "serve": "vue-cli-service serve --port 4200",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "prettier": "prettier '**/*.{js,json,css,scss,md,vue}' --write"
  },
  "dependencies": {
    "@amcharts/amcharts4": "^4.7.18",
    "@amcharts/amcharts4-geodata": "^4.1.9",
    "@aspnet/signalr": "^1.1.4",
    "@microsoft/signalr": "^3.1.5",
    "ant-design-vue": "^1.6.0",
    "axios": "^0.19.0",
    "bootstrap-vue": "^2.0.4",
    "clipboard": "^2.0.6",
    "clipboard-copy": "^3.1.0",
    "core-js": "^3.3.2",
    "echarts": "^4.4.0",
    "moment": "^2.24.0",
    "ramda": "^0.27.0",
    "vue": "^2.6.10",
    "vue-echarts": "^4.1.0",
    "vue-router": "^3.1.3",
    "vuex": "^3.0.1",
    "vuex-persistedstate": "^3.1.0"
  },
  "devDependencies": {
    "@babel/polyfill": "^7.6.0",
    "@ddmxh/prettier-config": "0.0.5",
    "@vue/cli-plugin-babel": "^4.0.0",
    "@vue/cli-plugin-eslint": "^4.0.0",
    "@vue/cli-plugin-router": "^4.0.0",
    "@vue/cli-plugin-vuex": "^4.0.0",
    "@vue/cli-service": "^4.0.0",
    "babel-eslint": "^10.0.3",
    "bootstrap": "^4.3.1",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.3.0",
    "mutationobserver-shim": "^0.3.3",
    "node-sass": "^4.14.1",
    "popper.js": "^1.15.0",
    "portal-vue": "^2.1.4",
    "prettier": "^2.1.1",
    "sass-loader": "^7.1.0",
    "vue-cli-plugin-bootstrap-vue": "^0.5.0",
    "vue-template-compiler": "^2.6.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "no-console": "off"
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
